        "statusBar.background": "#b35504",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#e56d05",
        "titleBar.activeBackground": "#b35504",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#b3550499",
        "titleBar.inactiveForeground": "#e7e7e799"
